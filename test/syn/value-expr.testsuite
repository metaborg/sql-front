testsuite Value Expressions
topsort ValExpr

test
  ['123 Main St.'] -> CharString("123 Main St.")

test
  ['Pig'] -> CharString("Pig")

test
  ['honey don''t'] -> CharString("honey don''t")

test
  ['honey don't'] fails

test
  ['honey don'''t'] fails

test
  ['honey don''''t'] succeeds

test
  "4" -> ExactNum("4")

test
  "7." -> ExactNum("7.")

test
  "-95.7" -> Minus(ExactNum("95.7"))

test
  "+364.5" -> Plus(ExactNum("364.5"))

test
  "0.0007" -> ExactNum("0.0007")

test
  ".6333" -> ExactNum(".6333")

test
  "4E3" -> ApproxNum("4E3")

test
  "-95.7E46" -> Minus(ApproxNum("95.7E46"))

test
  "+364E-5" -> Plus(ApproxNum("364E-5"))

test
  "0.7E1" -> ApproxNum("0.7E1")

test
  "-.15E-15" -> Minus(ApproxNum(".15E-15"))
